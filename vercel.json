{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/**",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://exclusive-omega-woad.vercel.app"
  }
}
